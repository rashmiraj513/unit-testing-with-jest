{
  "name": "unit-testing-with-jest",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --check \"**/*.{js,jsx,json,html,css,ts,tsx,yml,yaml}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,json,html,css,ts,tsx,yml,yaml}\" --ignore-unknown",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "npm run prettier:write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rashmiraj513/unit-testing-with-jest"
  },
  "keywords": [],
  "author": "Rashmi Raj",
  "license": "ISC",
  "description": "Testing with Jest",
  "devDependencies": {
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2"
  }
}
